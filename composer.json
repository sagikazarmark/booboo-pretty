{
    "name": "league/booboo-pretty",
    "description": "PrettyPage formatter for BooBoo",
    "license": "MIT",
    "keywords": ["pretty-page"],
    "homepage": "http://booboo.thephpleague.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sagikazarmark/booboo"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/booboo": "dev-inspector_improvements"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "patchwork/jsqueeze": "~1.0",
        "natxet/CssMin": "~3.0",
        "components/zepto": "1.1.6",
        "zeroclipboard/zeroclipboard": "2.2",
        "indigophp/robo-iless": "0.1.*@dev"
    },
    "autoload": {
        "psr-4": {
            "League\\BooBoo\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "config": {
        "component-dir": "vendor/assets"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
